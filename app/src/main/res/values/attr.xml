<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />

    <declare-styleable name="NodeWidget">
        <attr name="mode" format="enum">
            <enum name="normal" value="0" />
            <enum name="condition" value="1" />
            <enum name="common" value="2" />
            <enum name="optional" value="3" />
        </attr>
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="HelperCard">
        <attr name="icon" format="reference" />
        <attr name="title" />
    </declare-styleable>
</resources>