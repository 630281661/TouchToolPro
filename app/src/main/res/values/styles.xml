<resources>
    <color name="BooleanPinColor">#E81123</color>
    <color name="StringPinColor">#C239B3</color>
    <color name="IntegerPinColor">#00CC6A</color>
    <color name="ValueAreaPinColor">#10893E</color>
    <color name="ImagePinColor">#FF8C00</color>
    <color name="ColorPinColor">#F7630C</color>
    <color name="PointPinColor">#00B294</color>
    <color name="PathPinColor">#018574</color>
    <color name="WidgetPinColor">#847545</color>
    <color name="NodePinColor">#7E735F</color>
    <color name="AppPinColor">#4C4A48</color>
    <color name="AreaPinColor">#107C10</color>

    <color name="LauncherBackgroundColor">#FAFAFA</color>

    <attr name="colorPrimaryLight" format="color" />

    <style name="Theme.TouchTool.IconButton" parent="Widget.Material3.Button.TextButton">
        <item name="iconPadding">0dp</item>
        <item name="iconGravity">textStart</item>
        <item name="android:padding">0dp</item>
        <item name="android:textScaleX">0</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
    </style>

    <style name="Theme.TouchTool.IconButton.Outlined" parent="Theme.TouchTool.IconButton">
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">?attr/colorOutline</item>
    </style>

    <style name="Theme.TouchTool.IconButton.Filled" parent="Widget.Material3.Button">
        <item name="iconPadding">0dp</item>
        <item name="iconGravity">textStart</item>
        <item name="android:padding">0dp</item>
        <item name="android:textScaleX">0</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
    </style>

    <style name="Theme.TouchTool.Button.TextButton" parent="Widget.Material3.Button.TextButton">
        <item name="iconGravity">textTop</item>
        <item name="iconSize">24dp</item>
        <item name="iconPadding">8dp</item>
        <item name="iconTint">?attr/colorOnSurfaceVariant</item>
        <item name="android:textColor">?attr/colorOnSurfaceVariant</item>
    </style>

    <style name="Theme.TouchTool.ShapeAppearance.LeftCorner.Small" parent="ShapeAppearance.Material3.Corner.Small">
        <item name="cornerSizeTopRight">0%</item>
        <item name="cornerSizeBottomRight">0%</item>
    </style>

    <style name="Theme.TouchTool.ShapeAppearance.RightCorner.Small" parent="ShapeAppearance.Material3.Corner.Small">
        <item name="cornerSizeTopLeft">0%</item>
        <item name="cornerSizeBottomLeft">0%</item>
    </style>

    <style name="Theme.TouchTool.ShapeAppearance.TopCorner.Small" parent="ShapeAppearance.Material3.Corner.Small">
        <item name="cornerSizeTopRight">6dp</item>
        <item name="cornerSizeTopLeft">6dp</item>
        <item name="cornerSizeBottomLeft">0%</item>
        <item name="cornerSizeBottomRight">0%</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="Theme.TouchTool.ShapeAppearance.BottomCorner.Small" parent="ShapeAppearance.Material3.Corner.Small">
        <item name="cornerSizeTopRight">0%</item>
        <item name="cornerSizeTopLeft">0%</item>
        <item name="cornerSizeBottomLeft">6dp</item>
        <item name="cornerSizeBottomRight">6dp</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="Theme.TouchTool.TextAppearance.Hint" parent="TextAppearance.Material3.LabelSmall">
        <item name="android:textSize">5sp</item>
    </style>

    <style name="Theme.TouchTool.SideSheet" parent="ThemeOverlay.Material3.SideSheetDialog">
        <item name="sideSheetModalStyle">@style/Widget.Material3.SideSheet.Modal.Detached</item>
    </style>

</resources>